[user]
	name = Victor Johnson
	email = vicjohnson1213@gmail.com
[core]
	autocrlf = false
	safecrlf = true
	excludesfile = /Users/victor/.gitignore_global
	editor = vim
[alias]
	co = checkout
	ci = commit
    cia = commit -a -m 
	st = status
	br = branch
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	type = cat-file -t
	dump = cat-file -p
	dt = difftool
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
[difftool "nw"]
	cmd = ~/Documents/projects/difftool/Contents/MacOS/nwjs ~/Documents/projects/difftool $LOCAL $REMOTE
[diff]
	tool = nw
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[push]
	default = simple
[color]
	ui = true
[difftool]
	prompt = false
